html
  ::selection
    color:#fff;
    background:#162136;

  ::-moz-selection
    color:#fff;
    background:#162136;

.toprow
  margin:0px 0px 0px 0px;
  padding:0px 0px 10px 0px;

  width:100%;

  background:#2d426c;
  box-shadow:0px 4px 0px #162136;

a.toprowcontent
  color:#fff;

a:hover.toprowcontent
  text-decoration:none;
  color:#eff;

a:active.toprowcontent
  text-decoration:none;
  color:#eff;

.homedropdown
  width:40%;
  left:30%;
  right:30%;
